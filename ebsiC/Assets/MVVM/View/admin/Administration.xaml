<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ebsiC.Assets.MVVM.View.admin"
             xmlns:userControl="clr-namespace:ebsiC.Assets.MVVM.View.userControl"
             x:Class="ebsiC.Assets.MVVM.View.admin.Administration"
             mc:Ignorable="d" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             d:DesignHeight="450" d:DesignWidth="800">
    <Border Background="#323232"
            Margin="20"
            CornerRadius="5"
            Height="525"
            VerticalAlignment="Top">
        <userControl:CustomTabControl Tabs="{Binding adminTabs}" 
                                      SelectedTabIndex="{Binding CurrentIndex}"/>
    </Border>
</UserControl>
